@model PartyInvites.Models.GuestResponse

@{
    ViewBag.Title = "Результат";
} 

<!DOCTYPE html>
<html>
<head>
  <meta name="viewport" content="width=device-width" />
  <title>@ViewBag.Title</title>
</head>
<body>
    <b>@Model.Result</b>
    @if (ViewBag.Result == @Model.Result)
    {
        <b> , совпадает с заданным числом</b> 
    }
    else
    {
        <b> , не совпадает с заданным числом</b>    
    }
</body>
</html>